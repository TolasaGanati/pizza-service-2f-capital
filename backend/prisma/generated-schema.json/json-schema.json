          "originalType": "Int"
        },
        "orderItem": {
          "$ref": "#/definitions/OrderItem"
        },
        "topping": {
          "$ref": "#/definitions/Topping"
        }
      },
      "required": [
        "orderItemId",
        "toppingId"
      ]
