          "type": "string",
          "originalType": "String"
        },
        "address": {
          "type": "string",
          "originalType": "String"
        },
        "phone": {
          "type": [
            "string",
            "null"
          ],
          "originalType": "String"
        },
        "superAdminId": {
          "type": [
            "integer",
            "null"
          ],
          "originalType": "Int"
        },
        "createdAt": {
          "type": "string",
          "originalType": "DateTime",
          "format": "date-time"
        },
        "superAdmin": {
