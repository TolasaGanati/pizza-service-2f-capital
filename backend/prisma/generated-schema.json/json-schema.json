  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "restaurant": {
      "$ref": "#/definitions/Restaurant"
    },
    "pizza": {
      "$ref": "#/definitions/Pizza"
    },
    "topping": {
      "$ref": "#/definitions/Topping"
    },
    "pizzaTopping": {
      "$ref": "#/definitions/PizzaTopping"
    },
    "order": {
      "$ref": "#/definitions/Order"
    },
    "orderItem": {
