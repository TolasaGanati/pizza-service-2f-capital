        "password": {
          "type": "string",
          "originalType": "String"
        },
        "email": {
          "type": "string",
          "originalType": "String"
        },
        "role": {
          "type": "string",
          "originalType": "String"
        },
        "createdAt": {
          "type": "string",
          "originalType": "DateTime",
          "format": "date-time"
        },
        "restaurants": {
          "type": "array",
          "originalType": "Restaurant",
          "items": {
            "$ref": "#/definitions/Restaurant"
          }
        },
        "orders": {
          "type": "array",
          "originalType": "Order",
          "items": {
            "$ref": "#/definitions/Order"
          }
        }
